<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>3b8923b8-6b7c-4b50-bf7e-1bd1844528d7</Id>
  <Version>1.0.0.9</Version>
  <ProviderName>Data culture HSE</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="DC expert"/>
  <Description DefaultValue="DC expert — это надстройка Excel, которая позволяет использовать модели искусственного интеллекта прямо в электронных таблицах."/>
  <IconUrl DefaultValue="https://vvihsesoftware.github.io/dc-expert/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://vvihsesoftware.github.io/dc-expert/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://forms.yandex.ru/cloud/6981d7654936398717d6512b"/>
  <AppDomains>
    <AppDomain>https://llmexcel.liminity.se</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="SharedRuntime" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://vvihsesoftware.github.io/dc-expert/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <Runtimes>
          <Runtime resid="Taskpane.Url" lifetime="long"/>
        </Runtimes>
        <AllFormFactors>
          <ExtensionPoint xsi:type="CustomFunctions">
            <Script>
              <SourceLocation resid="Functions.Script.Url"/>
            </Script>
            <Page>
              <SourceLocation resid="Taskpane.Url"/>
            </Page>
            <Metadata>
              <SourceLocation resid="Functions.Metadata.Url"/>
            </Metadata>
            <Namespace resid="Functions.Namespace"/>
          </ExtensionPoint>
        </AllFormFactors>
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Taskpane.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://vvihsesoftware.github.io/dc-expert/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://vvihsesoftware.github.io/dc-expert/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://vvihsesoftware.github.io/dc-expert/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Functions.Script.Url" DefaultValue="https://vvihsesoftware.github.io/dc-expert/custom-functions.js"/>
        <bt:Url id="Functions.Metadata.Url" DefaultValue="https://vvihsesoftware.github.io/dc-expert/functions.json"/>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/liminityab/llmexcel"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://vvihsesoftware.github.io/dc-expert/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="Functions.Namespace" DefaultValue="DCexpert"/>
        <bt:String id="GetStarted.Title" DefaultValue="Начните работу с DC expert"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="AI Инструменты"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="DC expert"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="DC expert успешно загружен. Перейдите на вкладку ГЛАВНАЯ и нажмите кнопку 'DC expert', чтобы начать работу."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Нажмите, чтобы открыть DC expert"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>